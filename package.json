{
  "name": "evestory",
  "version": "0.0.1-canary.10",
  "private": true,
  "scripts": {
    "res:dev": "rescript build -w",
    "dev_": "concurrently -k 'bun run res:dev' 'prisma generate && next dev' 'email dev --dir ./src/emails --port 4000' 'prisma studio --browser none'",
    "dev": "concurrently -k 'bun run dune.watch' 'prisma generate && next dev' 'email dev --dir ./src/emails --port 4000' 'prisma studio --browser none'",
    "build": "prisma generate & rescript build -with-deps & next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:write": "prettier --write '**/*.{js,jsx,ts,tsx,mdx}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,mdx}' --cache",
    "prepare": "husky install",
    "bump": "bumpp",
    "email": "email dev --dir ./src/emails --port 4000",
    "prerelease": "bumpp prerelease --preid canary",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "opam.init": "opam update && opam switch create . 5.1.1 --deps-only --yes && opam install dune && dune build @install && opam install ocaml-lsp-server ocamlformat odoc utop --yes && opam install . --deps-only --yes && opam exec -- dune build",
    "dune.watch": "opam exec -- dune build -w",
    "dune.build": "opam exec -- dune build",
    "dune.sync": "opam exec -- dune build evestory.opam && opam exec -- dune build @install",
    "opam.update": "opam update",
    "opam.install": "opam install . --deps-only --with-test",
    "prisma": "prisma",
    "turbo": "turbo",
    "vercel": "vercel"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mdx}": [
      "prettier --write '**/*.{js,jsx,ts,tsx,mdx}' --cache",
      "eslint --fix"
    ]
  },
  "dependencies": {
    "@auth/prisma-adapter": "^1.0.8",
    "@glennsl/rescript-fetch": "^0.2.0",
    "@greenlabs/rescript-react-hook-form": "^0.11.2",
    "@hookform/resolvers": "^3.3.1",
    "@libsql/client": "^0.3.5",
    "@logsnag/next": "^1.0.2",
    "@mobily/ts-belt": "^3.13.1",
    "@planetscale/database": "^1.11.0",
