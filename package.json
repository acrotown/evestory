    "@vercel/edge-config": "^0.4.1",
    "autoprefixer": "10.4.14",
    "better-sqlite3": "^8.6.0",
    "class-variance-authority": "^0.6.0",
    "clsx": "^1.2.1",
    "crisp-sdk-web": "^1.0.21",
    "date-fns": "^2.30.0",
    "decco": "^1.6.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.5",
    "nanoid": "^5.0.1",
    "next": "^14.0.3",
    "next-auth": "^4.22.1",
