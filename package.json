{
  "name": "evestory",
  "version": "0.0.1-canary.10",
  "private": true,
  "scripts": {
    "res:dev": "rescript build -w",
    "dev.rescript": "concurrently -k 'pnpm run res:dev' 'next dev --turbo' 'pnpm run db:dev' 'pnpm run db:studio'",
    "dev.email": "email dev --dir ./src/emails --port 4000",
    "dev": "concurrently -k 'next dev --turbo' 'pnpm run db:dev' 'pnpm run db:studio'",
    "build:rescript": "rescript build -with-deps & next build",
    "build": "pnpm run db:generate & next build",
    "deploy:prod": "vercel --prod",
    "deploy:preview": "./scripts/deploy-preview.sh",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format:write": "prettier --write '**/*.{js,jsx,ts,tsx,mdx}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,mdx}' --cache",
    "prepare": "husky install",
    "bump": "bumpp",
    "email": "email",
    "prerelease": "bumpp prerelease --preid canary",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:push": "drizzle-kit push:sqlite",
    "db:check": "drizzle-kit check:sqlite",
    "db:studio": "drizzle-kit studio",
    "db:dev": "turso dev --db-file dev.db",
    "turbo": "turbo",
    "vercel": "vercel"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mdx}": [
      "prettier --write '**/*.{js,jsx,ts,tsx,mdx}' --cache",
      "eslint --fix"
    ]
  },
